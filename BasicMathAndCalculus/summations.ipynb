   "source": [
    "# summation of elements in python\n",
    "x = [1,2,3,4]\n",
    "n = len(x)\n",
    "\n",
    "summation = sum(10*x[i] for i in range(0,n))\n",
    "print(summation)\n",
    "\n",
    "# summation of elements using sympy\n",
    "\n",
    "from sympy import *\n",
    "\n",
    "i,n = symbols('i n')\n",
    "\n",
    "sumSympy = Sum(2*i, (i,1,n))\n",
    "\n",
    "sumUpto5 = sumSympy.subs(n,5)\n",
    "print(sumUpto5.doit())"
   ]
